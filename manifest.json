{
  "name": "Drumpfinator",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Drumpfinate the web",
  "homepage_url": "https://github.com/mark-monteiro/drumpfinator",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options/index.html",
  "permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["js/jquery/jquery.js", "src/inject/inject.js"]
    }
  ]
}